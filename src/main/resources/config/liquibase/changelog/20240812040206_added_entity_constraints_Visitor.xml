    <!--
        Added the constraints for entity Visitor.
    -->
    <changeSet id="20240812040206-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="address_id"
                                 baseTableName="visitor"
                                 constraintName="fk_visitor__address_id"
